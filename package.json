    "@architect/inventory": "^1.2.3",
    "@architect/sandbox": "^3.3.7",
    "esbuild": "^0.8.54",
    "eslint": "^7.21.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2"
  }
}
